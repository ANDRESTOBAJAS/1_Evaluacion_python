#este programa 
def suma_par():
  num_final=input("dime hasta que numero quieres que sume: ")
  suma_par=0
  suma_impar=0
  for cont in range(1,int(num_final)+1):
    if(cont%2==0):
      suma_par=suma_par+cont
    else:
      suma_impar=suma_impar+cont
   
  print("suma acumulada = "+str(suma_par))
  
suma_par()
